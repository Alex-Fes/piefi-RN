{
  "expo": {
    "name": "Piefi",
    "slug": "Piefi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mainIcon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splashLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mainLogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/mainIcon.png"
    }
  }
}
